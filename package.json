{
	"name": "laravel-goto-facade",
	"displayName": "Laravel Goto Facade",
	"description": "Quick jump to facade",
	"version": "0.0.3",
	"publisher": "enxas-laravel-goto-facade",
    "license": "MIT",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/enxas/laravel-goto-facade.git"
	},
	"engines": {
		"vscode": "^1.96.0"
	},
	"categories": [
        "Other"
    ],
    "keywords": [
        "PHP",
        "Laravel"
    ],
	"activationEvents": [ "onLanguage:php" ],
	"main": "./out/extension.js",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.96.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"eslint": "^9.16.0",
		"typescript": "^5.7.2",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1"
	}
}
